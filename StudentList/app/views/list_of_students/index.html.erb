<p id="notice"><%= notice %></p>

<h1>List Of Students</h1>

<table>
  <thead>
    <tr>
      <th>Src</th>
      <th>Lang</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_of_students.each do |list_of_student| %>
      <tr>
        <td><%= list_of_student.src %></td>
        <td><%= list_of_student.lang %></td>
        <td><%= list_of_student.desc %></td>
        <td><%= link_to 'Show', list_of_student %></td>
        <td><%= link_to 'Edit', edit_list_of_student_path(list_of_student) %></td>
        <td><%= link_to 'Destroy', list_of_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List Of Student', new_list_of_student_path %>
